    "cross-env": "^5.2.0",
    "laravel-mix": "^4.0.4",
    "resolve-url-loader": "^2.3.1",
    "sass": "^1.15.2",
    "sass-loader": "^7.1.0",
    "vue-template-compiler": "^2.6.10"
  },
  "dependencies": {
    "breakpoint-sass": "^2.7.1",
    "jquery": "^3.4.1"
  }
